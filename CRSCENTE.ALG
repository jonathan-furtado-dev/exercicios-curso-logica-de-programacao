Algoritmo "crscente"


Var

    x , y : real



Inicio

   escreva("Digite dois números: ")
   leia(x)
   leia(y)



   enquanto x <> y faca

      se x > Y entao
         escreval("Decrescente")
         
      senao
         escreval("Crescente")
      fimse
      escreval("Digite outros dois números")
      leia(x)
      leia(y)

   fimenquanto

Fimalgoritmo